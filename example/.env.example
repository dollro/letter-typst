# GitLab project access token for cloning letter-typst (read_repository scope)
LETTER_TYPST_GL_TOKEN=
# Git ref (branch/tag) to bake into image
LETTER_TYPST_GL_BRANCH=main
# GitLab host (without https://)
LETTER_TYPST_GL_HOST=gitlab.example.com
# GitLab project path (without .git suffix)
LETTER_TYPST_GL_PROJECT=namespace/letter-typst
# Typst package name (must match @local import in .typ files)
LETTER_TYPST_NAME=letter-typst
# Package version (set for release tarball, leave empty for dev/clone from REF)
LETTER_TYPST_VERSION=0.1.0
# Typst compiler version (git tag)
TYPST_VERSION=v0.14.2
